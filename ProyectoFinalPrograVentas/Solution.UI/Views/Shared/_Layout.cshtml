<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Menú</a>

                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav flex-grow-1">

                        <li class="nav-item">
                            @if (User.Identity.IsAuthenticated.Equals(true) && User.Identity.Name.Equals("Admin@hotmail.com"))
                            {
                                <a class="nav-link text-dark" asp-area="" asp-controller="Producto" asp-action="Index">Producto</a>
                            }
                        </li>

                        <li class="nav-item">
                            @if (User.Identity.IsAuthenticated.Equals(true) && User.Identity.Name.Equals("Admin@hotmail.com"))
                            {
                                <a class="nav-link text-dark" asp-area="" asp-controller="Factura" asp-action="Index">Factura</a>
                            }
                        </li>
                        <li class="nav-item">
                            @if (User.Identity.IsAuthenticated.Equals(true) && !User.Identity.Name.Equals("Admin@hotmail.com"))
                            {
                                <a class="nav-link text-dark" asp-area="" asp-controller="Carrito" asp-action="Index">Carrito</a>
                            }
                        </li>

                        @*<div class="nav-item">
                                <a class="nav-link" href='/Identity/Account/Login'>Login</a>
                            </div>*@
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()

        </main>
    </div>

